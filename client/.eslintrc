{
  parser: "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "env": {
      "browser": true,
      "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "react/no-unused-prop-types": [2, { skipShapeProps: true }],
    "class-methods-use-this": [0],
    "no-shadow": [0],
  }
}